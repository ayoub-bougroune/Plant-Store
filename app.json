{
  "expo": {
    "name": "Plant Shop",
    "slug": "Plant-Shop",
    "description": "this my first React Native app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./app/assets/plant1.png",
    "splash": {
      "image": "./app/assets/plant1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
